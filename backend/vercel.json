{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/api/index.py",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/favicon.png",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py",
      "headers": {
        "Access-Control-Allow-Origin": "https://a-closer-look-topaz.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-Request-ID",
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Max-Age": "3600"
      }
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
